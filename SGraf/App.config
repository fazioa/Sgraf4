            </setting>
            <setting name="contenutoDettaglio" serializeAs="String">
                <value>Contiene fotografie di....</value>
            </setting>
            <setting name="autore" serializeAs="String">
                <value>nome e cognome autore</value>
            </setting>
            <setting name="luogo" serializeAs="String">
                <value>comune</value>
            </setting>
            <setting name="gradoCognomeNome" serializeAs="String">
                <value />
            </setting>
            <setting name="carattereDimensioneTitoloImmagine" serializeAs="String">
                <value>20</value>
            </setting>
            <setting name="titoloFoto" serializeAs="String">
                <value>Immagine nr.</value>
            </setting>
            <setting name="bIncorporaImmagini" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="disposizioneColonne" serializeAs="String">
                <value>1</value>
            </setting>
            <setting name="disposizioneRighe" serializeAs="String">
                <value>2</value>
            </setting>
            <setting name="carattereDimensioneDidascalia" serializeAs="String">
                <value>9</value>
            </setting>
            <setting name="fotoLarghezzaCM" serializeAs="String">
                <value>15</value>
            </setting>
            <setting name="fotoAltezzaCM" serializeAs="String">
                <value>10</value>
            </setting>
            <setting name="tipoFascicolo" serializeAs="String">
                <value>0</value>
            </setting>
            <setting name="newVersion" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="bMemorizzaDidascalia" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="fotoAltezzaThumb" serializeAs="String">
                <value>151</value>
            </setting>
            <setting name="bEXIFMarca" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="bEXIFModello" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="bEXIFEsposizione" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="bEXIFDiaframma" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="bEXIFDataOra" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="bEXIFFlash" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="bEXIFISO" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="coloreSelezione" serializeAs="String">
                <value>192, 255, 255</value>
            </setting>
            <setting name="trasparenzaImmagine" serializeAs="String">
                <value>-40</value>
            </setting>
            <setting name="bNomeFile" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="zoomMiniaturaDefaultMenu" serializeAs="String">
                <value>30</value>
            </setting>
            <setting name="zoomMiniaturaDefaultMouse" serializeAs="String">
                <value>3</value>
            </setting>
            <setting name="thumbnailDisplayResolution" serializeAs="String">
                <value>200</value>
            </setting>
            <setting name="doc_nomeModello" serializeAs="String">
                <value>fascicolo fotografico.dotx</value>
            </setting>
            <setting name="segnalibro_autore" serializeAs="String">
                <value>autore</value>
            </setting>
            <setting name="segnalibro_intestazione1" serializeAs="String">
                <value>intestazione1</value>
            </setting>
            <setting name="segnalibro_intestazione2" serializeAs="String">
                <value>intestazione2</value>
            </setting>
            <setting name="segnalibro_intestazione3" serializeAs="String">
                <value>intestazione3</value>
            </setting>
            <setting name="segnalibro_oggetto" serializeAs="String">
                <value>oggetto</value>
            </setting>
            <setting name="segnalibro_contenutoDettaglio" serializeAs="String">
                <value>contenutoDettaglio</value>
            </setting>
            <setting name="segnalibro_luogo_data" serializeAs="String">
                <value>luogo_data</value>
            </setting>
            <setting name="carattereFont" serializeAs="String">
                <value>-1</value>
            </setting>
            <setting name="carattereDimensioneNomeFile" serializeAs="String">
                <value>12</value>
            </setting>
            <setting name="carattereDimensioneDatiEXIF" serializeAs="String">
                <value>8</value>
            </setting>
            <setting name="fotoLarghezzaThumb" serializeAs="String">
                <value>400</value>
            </setting>
            <setting name="fotoLarghezzaThumbMin" serializeAs="String">
                <value>180</value>
            </setting>
            <setting name="doc_savedDocumentName" serializeAs="String">
                <value>fascicolo.doc</value>
            </setting>
            <setting name="titoloLegenda" serializeAs="String">
                <value>Legenda</value>
            </setting>
            <setting name="carattereDimensioneLegenda" serializeAs="String">
                <value>12</value>
            </setting>
            <setting name="doc_modelPath" serializeAs="String">
                <value>modello</value>
            </setting>
            <setting name="doc_resultPath" serializeAs="String">
                <value>result</value>
            </setting>
            <setting name="print_dpi" serializeAs="String">
                <value>150</value>
            </setting>
        </SGraf.MySettings>
    </userSettings>

	<log4net>
		<appender name="TestAppender" type="log4net.Appender.RollingFileAppender">
			<file value="Xlogs.log" />
			<encoding value="utf-8" />
			<appendToFile value="true" />
			<rollingStyle value="Date" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date- %message%n" />
			</layout>
		</appender>
		<root>
			<level value="All" />
			<!-- If the following line is not included the log file will not be created even if log4net is configured with this file. -->
			<appender-ref ref="TestAppender" />
		</root>
	</log4net>

</configuration>